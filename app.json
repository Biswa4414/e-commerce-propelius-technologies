{
  "expo": {
    "name": "e-commerce",
    "slug": "e-commerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "ecommerce",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.biswa4414.ecommerce",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.biswa4414.ecommerce",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-speech-recognition",
        {
          "microphonePermission": "Allow $(BUNDLE_DISPLAY_NAME) to use the microphone for voice search.",
          "speechRecognitionPermission": "Allow $(BUNDLE_DISPLAY_NAME) to use speech recognition for voice search."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d6e4affa-6ed0-4fd2-86ad-9345486c36e6"
      }
    }
  }
}
